{
  "expo": {
    "name": "Solvend",
    "slug": "Solvend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/solvend.png",
    "scheme": "privy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.abkgami.privy",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCl0C26EuumGSi2C3f1jO7YHAfxNuGG0rg"
        }
      }
    },
    "extra": {
      "privyAppId": "cmfvrbc6u00bbjp0ev75fcs0r",
      "privyClientId": "client-WY6QkUmS7gAv1XF1vsBm7BhpxECzzzMPn414K8yiuUKZ3"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
